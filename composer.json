{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4.3",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/recipe-blog": "^1.1",
        "tractorcow/silverstripe-imagegallery": "dev-master",
        "symbiote/silverstripe-multivaluefield": "^5.0",
        "undefinedoffset/sortablegridfield": "^2.0",
        "lekoala/silverstripe-debugbar": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "files": [
            "app/lib/loader.php",
            "app/src/pagetypes/Page.php",
            "app/src/controllers/PageController.php"
        ],
        "psr-4": {
            "TractorCow\\ImageGallery\\": "./vendor/silverstripe/image_gallery/src/"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/silverstripe/{$name}": [
                "tractorcow/silverstripe-imagegallery"
            ]
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/blog.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "scripts": {
        "post-update-cmd": [
            "@php -r \"file_exists('app/_config.php') || copy('app/_config.example.php', 'app/_config.php');\"",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
