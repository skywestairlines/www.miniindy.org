{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
        "silverstripe/cms": "^3.7",
        "silverstripe/framework": "^3.7",
        "silverstripe/reports": "^3.7",
        "silverstripe/siteconfig": "^3.7",
        "silverstripe-themes/simple": "3.1.*",

        "silverstripe-labs/googleanalytics": "*",
        "wilr/silverstripe-googlesitemaps": "*",
        "tractorcow/silverstripe-imagegallery": "*",
        "silverstripe/forum": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5",
        "lekoala/silverstripe-debugbar": "*"
    },
    "extra": {
        "branch-alias": {
            "3.x-dev": "3.6.x-dev"
        },
        "installer-paths": {
            "vendor/{$name}": [
                "silverstripe-labs/googleanalytics",
                "wilr/silverstripe-googlesitemaps",
                "tractorcow/silverstripe-imagegallery",
                "silverstripe-labs/youtubeservice",
                "silverstripe/forum",
                "lekoala/silverstripe-debugbar"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "modules/",
            "vendor/"
        ],
        "files": [
            "modules/autoloader.php"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
