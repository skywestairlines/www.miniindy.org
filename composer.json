{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
        "silverstripe/cms": "3.6.3@stable",
        "silverstripe/framework": "3.6.3@stable",
        "silverstripe/reports": "3.6.3@stable",
        "silverstripe/siteconfig": "3.6.3@stable",
        "silverstripe-themes/simple": "3.1.*",

        "silverstripe-labs/googleanalytics": "dev-master",
        "wilr/silverstripe-googlesitemaps": "^1.6",
        "tractorcow/silverstripe-imagegallery": "^3",
        "silverstripe/forum": "dev-master"

    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5",
        "lekoala/silverstripe-debugbar": "^1"
    },
    "extra": {
        "branch-alias": {
            "3.x-dev": "3.6.x-dev"
        },
        "installer-paths": {
            "vendor/{$name}": [
                "silverstripe-labs/googleanalytics",
                "wilr/silverstripe-googlesitemaps",
                "tractorcow/silverstripe-imagegallery",
                "silverstripe-labs/youtubeservice",
                "silverstripe/forum",

                "lekoala/silverstripe-debugbar"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "modules/",
            "vendor/"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}